{
  "name": "pakframe",
  "description": "⚛️ A lightweight, reactive TypeScript framework for building dynamic web interfaces.",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "author": "thednp",
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build__": "rolldown --config rolldown.config.ts",
    "build": "tsdown && pnpm build-plugin",
    "build-plugin": "tsdown --config vite/tsdown.config.ts",
    "build_": "tsc && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/node": "^24.0.14",
    "rolldown": "1.0.0-beta.27",
    "rolldown-plugin-dts": "^0.13.14",
    "tsdown": "^0.12.9",
    "typescript": "~5.8.3",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "@thednp/domparser": "^0.1.5",
    "csstype": "^3.1.3",
    "pakframe": "link:"
  },
  "peerDependencies": {
    "vite": "^6.3.5"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./jsx-dev-runtime": {
      "import": "./dist/jsx-dev-runtime.js",
      "require": "./dist/jsx-dev-runtime.cjs"
    },
    "./jsx-runtime": {
      "import": "./dist/jsx-runtime.js",
      "require": "./dist/jsx-runtime.cjs"
    },
    "./meta": {
      "import": "./dist/meta.js",
      "require": "./dist/meta.cjs"
    },
    "./router": {
      "import": "./dist/router.js",
      "require": "./dist/router.cjs"
    },
    "./ssr": {
      "import": "./dist/ssr.js",
      "require": "./dist/ssr.cjs"
    },
    "./package.json": "./package.json"
  }
}
